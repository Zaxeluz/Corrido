        <Slider x:Name="sldPC" HorizontalAlignment="Right" Margin="0,130,100,0" VerticalAlignment="Top" Width="20" Orientation="Vertical" Height="394" ValueChanged="sldPC_ValueChanged" Maximum="100"/>
        <TextBlock x:Name="lblTiempoTranscurrido" HorizontalAlignment="Left" Margin="434,268,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="116" Width="241" FontSize="36" Text="0" />
